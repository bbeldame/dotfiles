{
	"Print to console": {
		"prefix": "csl",
		"body": [
			"console.log('$1');"
		],
		"description": "Log output to console"
	},
	"Print to console descriptive": {
		"prefix": "csld",
		"body": [
			"console.log('$1', $1);"
		],
		"description": "Log output to console descriptively"
	},
	"Use a deep console log": {
		"prefix": "csdeep",
		"body": [
			"console.log('$1', JSON.stringify($1, null, 4));"
		],
		"description": "Log output to console with max depth in object"
	},
	"Console log of time in the line": {
		"prefix": "cstime",
		"body": [
			"console.log('$TM_FILENAME_BASE $TM_LINE_NUMBER', new Date().toTimeString().split(' ')[0], new Date().getMilliseconds());"
		],
		"description": "Print console log with time on the line"
	},
